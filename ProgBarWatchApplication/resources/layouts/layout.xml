<?xml version="1.0" ?>
<layout id="WatchFace">
	<drawable class="Background" />
	<label id="HourLabel" text="@Strings.HourLabel" x="45" y="0" font="Gfx.FONT_SMALL"
		justification="Gfx.TEXT_JUSTIFY_RIGHT" color="Gfx.COLOR_BLUE" />
	
	<label id="MinuteLabel" text="@Strings.MinuteLabel" x="45" y="20" font="Gfx.FONT_SMALL"
		justification="Gfx.TEXT_JUSTIFY_RIGHT" color="Gfx.COLOR_BLUE" />
	
	<label id="MonthLabel" text="@Strings.MonthLabel" x="45" y="40" font="Gfx.FONT_SMALL"
		justification="Gfx.TEXT_JUSTIFY_RIGHT" color="Gfx.COLOR_BLUE" />
	
	<label id="DayLabel" text="@Strings.DayLabel" x="45" y="60" font="Gfx.FONT_SMALL"
		justification="Gfx.TEXT_JUSTIFY_RIGHT" color="Gfx.COLOR_BLUE" />
		
	<label id="StepsLabel" text="@Strings.StepsLabel" x="45" y="80" font="Gfx.FONT_SMALL"
		justification="Gfx.TEXT_JUSTIFY_RIGHT" color="Gfx.COLOR_BLUE" />
	
	<label id="BatteryLabel" text="@Strings.BatteryLabel" x="45" y="100" font="Gfx.FONT_SMALL"
		justification="Gfx.TEXT_JUSTIFY_RIGHT" color="Gfx.COLOR_BLUE" />
		
	<drawable id="HourBar" class="ProgressBars.ProgressBar">
		<param name="x">55</param>
		<param name="y">3</param>
		<param name="segments">24</param>
		<param name="length">145</param>
		<param name="thickness">15</param>
		<param name="orientation">"horizontal"</param>
	</drawable>
	
	
	<drawable id="MinuteBar" class="ProgressBars.ProgressBar">
        <param name="x">55</param>
        <param name="y">23</param>
        <param name="segments">60</param>
        <param name="length">145</param>
		<param name="thickness">15</param>
		<param name="orientation">"horizontal"</param>
    </drawable>
    
    <drawable id="MonthBar" class="ProgressBars.ProgressBar">
        <param name="x">55</param>
        <param name="y">43</param>
        <param name="segments">12</param>
        <param name="length">145</param>
		<param name="thickness">15</param>
		<param name="orientation">"horizontal"</param>
    </drawable>
    
    <drawable id="DayBar" class="ProgressBars.ProgressBar">
        <param name="x">55</param>
        <param name="y">63</param>
        <param name="segments">31</param>
        <param name="length">145</param>
		<param name="thickness">15</param>
		<param name="orientation">"horizontal"</param>
    </drawable>
    
    
    <drawable id="StepsBar" class="ProgressBars.ProgressBar">
        <param name="x">55</param>
        <param name="y">83</param>
        <param name="segments">100</param>
        <param name="length">145</param>
		<param name="thickness">15</param>
		<param name="orientation">"horizontal"</param>
    </drawable>
    
    <drawable id="BatteryBar" class="ProgressBars.ProgressBar">
        <param name="x">55</param>
        <param name="y">103</param>
        <param name="segments">100</param>
        <param name="length">145</param>
		<param name="thickness">15</param>
		<param name="orientation">"horizontal"</param>
    </drawable>
   
     
    <drawable id="BluetoothNotification" class="NotificationIcons.NotificationIcon">
        <param name="x">55</param>
        <param name="y">123</param>
        <param name="image">Drawables.BluetoothIcon</param>
    </drawable>
    
    <drawable id="MessageNotification" class="NotificationIcons.NotificationIcon">
        <param name="x">75</param>
        <param name="y">123</param>
        <param name="image">Drawables.MessageIcon</param>
    </drawable>
    
    <drawable id="AlarmNotification" class="NotificationIcons.NotificationIcon">
        <param name="x">95</param>
        <param name="y">123</param>
        <param name="image">Drawables.AlarmIcon</param>
    </drawable>
    
    <drawable id="MoveNotification" class="NotificationIcons.NotificationIcon">
        <param name="x">115</param>
        <param name="y">123</param>
        <param name="image">Drawables.MoveIcon</param>
    </drawable>
     
</layout>